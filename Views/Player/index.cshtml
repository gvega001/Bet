@model Bet.Models.ViewModels.PlayerViewModels

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<div>
    <h4>Player</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Model.Player.FirstName)
        </dt>

        <dd>
            @Model.Player.LastName)
        </dd>

        <dt>
            @Model.Player.Bets)
        </dt>

        <dd>
            @Model.Player.MembershipId)
        </dd>

        <dt>
            @Model.Player.IsSubscribed)
        </dt>

        <dd>
            @Model.Player.DateOfBirth)
        </dd>

        <dt>
            @Model.Player.Email)
        </dt>

        <dd>
            @Model.Player.)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
